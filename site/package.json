{
  "name": "truefit-react-template",
  "scripts": {
    "start": "yarn run generateAllFiles && yarn serve",
    "serve": "yarn webpack-dev-server --config webpack.config.dev.ts",
    "lint": "eslint 'app/**/*.{ts,tsx}' 'webpack.config.*' --color --fix",
    "type-check": "yarn run tsc",
    "pretty": "prettier --write '**/*.{ts,tsx,scss}'",
    "webpack": "webpack --config webpack.config.prod.ts --bail",
    "clean": "rimraf ./lib",
    "build": "yarn run generateAllFiles && yarn run webpack",
    "generateAllFiles": "wall-e g -actions -components -constants -services -selectors -types -reducers -rootReducer -utils",
    "generateActionIndexFiles": "wall-e g -actions",
    "generateComponentIndexFiles": "wall-e g -components",
    "generateConstantIndexFiles": "wall-e g -constants",
    "generateServiceIndexFiles": "wall-e g -services",
    "generateSelectorIndexFiles": "wall-e g -selectors",
    "generateTypesIndexFiles": "wall-e g -types",
    "generateReducerIndexFiles": "wall-e g -reducers",
    "generateRootReducer": "wall-e g -rootReducer",
    "generateUtilsIndexFiles": "wall-e g -utils",
    "todo": "grep --color --exclude-dir={.git,node_modules,lib} --exclude={package.json,yarn-error.log,README.md} -rEI 'TODO|FIXME' . 2>/dev/null"
  },
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --color --fix",
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@date-io/core": "^1.3.6",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.9.0",
    "@reduxjs/toolkit": "^1.2.5",
    "@truefit/bach": "^2.0.1",
    "@truefit/bach-material-ui": "^2.0.0",
    "@truefit/bach-react-hook-form": "^2.0.1",
    "@truefit/bach-react-router": "^2.0.1",
    "@truefit/bach-redux": "^2.0.1",
    "@truefit/http-utils": "^2.0.2",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "date-fns": "^2.10.0",
    "formik": "^2.1.4",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "notistack": "^0.9.9",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hook-form": "^5.2.0",
