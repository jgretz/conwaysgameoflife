    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.149",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
